<Window x:Class="OS_Simulator.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.codeplex.com"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:winformchart="clr-namespace:System.Windows.Forms.DataVisualization.Charting;assembly=System.Windows.Forms.DataVisualization"
        Title="Summary" Height="480" Width="660" WindowState="Maximized">
    <Grid>
        <oxy:Plot x:Name="Results" Title="Performance Graph" Model="{Binding PlotModel}" Margin="10" Grid.Row="1">
        </oxy:Plot>
        <DockPanel LastChildFill="True">
            <WindowsFormsHost x:Name="host" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                <winformchart:Chart x:Name="Graph" Dock="Fill">
                    <winformchart:Chart.ChartAreas>
                        <winformchart:ChartArea Name="ChartArea"/>
                    </winformchart:Chart.ChartAreas>
                </winformchart:Chart>    
            </WindowsFormsHost>
        </DockPanel>


    </Grid>

</Window>